{
  "ceph_conf_overrides": {
    "global": {
      "osd_pool_default_pg_num": 12,
      "osd_pool_default_size": 1,
      "debug_ms": 20
    },
    "osd": {
      "debug_bluefs": 20,
      "debug_bluestore": 20
    }
  },
  "cephfs_pools": [
    {
      "name": "cephfs_metadata",
      "pgs": 8,
      "size": 1
    },
    {
      "name": "cephfs_data",
      "pgs": 8,
      "size": 1
    }
  ],
  "ceph_mon_docker_memory_limit": "2g"
}
